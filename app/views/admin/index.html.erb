
<div id="page-wrapper">
<div class="lefnavspace">
    <div class="container-fluid">

        <!-- Page Heading -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">
                    내 사물함 <small>My Place</small>
                </h1>
                <ol class="breadcrumb">
                    <li class="active">
                        이용에 감사드립니다!
                    </li>
                </ol>
            </div>
        </div>
        <!-- /.row -->

        <div class="row">
            <div class="col-lg-12">
                <div class="alert alert-info alert-dismissable">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                    <i class="fa fa-info-circle"></i>  <strong>이용방법</strong>이 궁금하시다면 <strong><a href="">이 곳</a></strong>을 클릭하여 주세요
                </div>
            </div>
        </div>
        <!-- /.row -->

        <div class="row">
            <div class="col-lg-6 col-md-6">
                <div class="panel">
                    <div class="panel-heading">
                        <div class="row">
                            <div class="col-xs-3">
                                <i class="fa fa-tasks fa-5x"></i>
                            </div>
                            <div class="col-xs-9 text-right">
                                <div class="huge">사물함 관리</div>
                                <div>경희대학교 응용화학과</div>
                           </div>
                        </div>
                    </div>
                    <a href="#">
                        <div class="panel-footer">
                            <span class="pull-left">자세히 보기</span>
                            <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                            <div class="clearfix"></div>
                        </div>
                    </a>
                </div>
            </div>
            <div class="col-lg-6 col-md-6">
                <div class="panel">
                    <div class="panel-heading">
                        <div class="row">
                            <div class="col-xs-3">
                                <i class="fa fa-tasks fa-5x"></i>
                            </div>
                            <div class="col-xs-9 text-right">
                                <div class="huge">접수현황보기</div>
                                <div>경희대학교 공과대학 4층</div>
                                        <span class="label label-success">접수중</span>
                                <span class="label label-danger">접수완료</span>
                                <span class="label label-warning">정정기간</span>
                            </div>
                        </div>
                    </div>
                    <a href="#">
                        <div class="panel-footer">
                            <span class="pull-left">자세히 보기</span>
                            <span class="pull-right"><i class="fa fa-arrow-circle-right"></i></span>
                            <div class="clearfix"></div>
                        </div>
                    </a>
                </div>
            </div>
</div>
           <!--userlist-->
           <div class="row">
               <div class="col-lg-12">
                       <h1 class="page-header">
                           회원목록 <small>User list</small>
                       </h1>

                               <%= flash[:notice] %>
                               <%= flash[:caution] %>
                               <table class="table table-hover">
                                 <thead>
                                   <tr>
                                     <th>이름</th>
                                     <th>학번</th>
                                     <th>이메일</th>
                                     <th>신청한 사물함</th>
                                   </tr>
                                 </thead>
                                 <tbody>
                                   <% @user_admin.each do |u_i| %>
                                     <tr>
                                       <td>
                                         <%= u_i.name %>&nbsp
                                         <% if u_i.admin == true %>
                                           <%= link_to "[전체삭제]", "/destroy_all/:u_i.id", method:"delete" %>
                                         <% else %>
                                           <%= link_to "[삭제]", admin_path(u_i.id), method: "delete" %>
                                         <% end %>
                                       </td>
                                       <td><%= u_i.stuN %></td>
                                       <td><%= u_i.email %></td>
                                       <% if u_i.seatNumber == nil%>
                                         <td>미신청</td>
                                       <% else %>
                                         <td><%= u_i.seatNumber %></td>
                                       <% end %>
                                     </tr>
                                   <% end %>
                                 </tbody>
                              </table>
               </div>
           </div>
                   <!--row-->
           <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h3 class="panel-title"><i class="fa fa-clock-o fa-fw"></i> Tasks Panel</h3>
                    </div>
                    <div class="panel-body">
                        <div class="list-group">
                            <a href="#" class="list-group-item">
                                <span class="badge">just now</span>
                                <i class="fa fa-fw fa-calendar"></i> Calendar updated
                            </a>
                            <a href="#" class="list-group-item">
                                <span class="badge">4 minutes ago</span>
                                <i class="fa fa-fw fa-comment"></i> Commented on a post
                            </a>
                            <a href="#" class="list-group-item">
                                <span class="badge">23 minutes ago</span>
                                <i class="fa fa-fw fa-truck"></i> Order 392 shipped
                            </a>
                            <a href="#" class="list-group-item">
                                <span class="badge">46 minutes ago</span>
                                <i class="fa fa-fw fa-money"></i> Invoice 653 has been paid
                            </a>
                            <a href="#" class="list-group-item">
                                <span class="badge">1 hour ago</span>
                                <i class="fa fa-fw fa-user"></i> A new user has been added
                            </a>
                            <a href="#" class="list-group-item">
                                <span class="badge">2 hours ago</span>
                                <i class="fa fa-fw fa-check"></i> Completed task: "pick up dry cleaning"
                            </a>
                            <a href="#" class="list-group-item">
                                <span class="badge">yesterday</span>
                                <i class="fa fa-fw fa-globe"></i> Saved the world
                            </a>
                            <a href="#" class="list-group-item">
                                <span class="badge">two days ago</span>
                                <i class="fa fa-fw fa-check"></i> Completed task: "fix error on sales page"
                            </a>
                        </div>
                        <div class="text-right">
                            <a href="#">View All Activity <i class="fa fa-arrow-circle-right"></i></a>
                        </div>
                    </div>
                </div>
            </div>
</div>
        <!-- /.row -->

    </div>
    <!-- /.container-fluid -->
</div>
</div>
<!-- /#page-wrapper -->
